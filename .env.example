# Binance API Credentials (required for Positions view)
# Get your API keys from: https://www.binance.com/en/my/settings/api-management
# For testnet: https://testnet.binance.vision/
BINANCE_API_KEY=your_binance_api_key
BINANCE_API_SECRET=your_binance_api_secret

# Binance Testnet Mode (optional, defaults to false)
# When enabled, uses testnet endpoints instead of production
# WARNING: Requires testnet API keys, not production keys
BINANCE_USE_TESTNET=false

# News API Key (required for News tab)
# NewsData.io - https://newsdata.io/documentation
NEWSDATA_API_KEY=your_newsdata_io_api_key
